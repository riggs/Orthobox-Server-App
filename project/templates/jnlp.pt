<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://staging.xlms.org:8128" href="jnlp/${session}.jnlp">
    <information>
        <title>Orthobox xlms client app</title>
        <vendor>Synaptic Design</vendor>
        <offline-allowed/>
    </information>
    <resources>
        <!-- Application Resources -->
        <j2se version="1.7+" href= "http://java.sun.com/products/autodl/j2se"/>
        <jar href="jar/orthobox.jar"
             main="true" />

    </resources>
    <application-desc
            name="Orthobox Client App"
            main-class="com.synapticdesign.orthobox.OrthoBox"
            width="300"
            height="400">
    </application-desc>
    <update check="background"/>
    <security>
        <all-permissions/>
    </security>
    <argument>${url}</argument><!--UUID -->
    <argument>${session}.mp4</argument><!--FILENAME -->
</jnlp>
